{
  "name": "@auralous/ui",
  "version": "0.0.0",
  "description": "Shared UI for Auralous",
  "scripts": {
    "build:dts": "tsc",
    "build:babel": "babel src -d dist --extensions \".ts,.tsx\" --copy-files",
    "build": "yarn build:dts && yarn build:babel",
    "build:watch": "concurrently \"yarn build:dts --watch\" \"yarn build:babel -w\"",
    "clean": "rm -rf dist && rm tsconfig.tsbuildinfo"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/auralous/auralous.git",
    "directory": "ui"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auralous/auralous/issues"
  },
  "homepage": "https://github.com/auralous/auralous#readme",
  "dependencies": {
    "@auralous/api": "^0.0.0",
    "@auralous/player": "^0.0.0",
    "mitt": "^3.0.0",
    "react-native-capsize": "^0.2.2",
    "react-native-image-colors": "^1.4.0",
    "recyclerlistview": "^3.0.5"
  },
  "devDependencies": {
    "@fluentui/react-context-selector": "*",
    "@types/react": "*",
    "@types/react-native": "*",
    "react": "*",
    "react-i18next": "*",
    "react-native-gesture-handler": "*",
    "react-native-reanimated": "*",
    "react-native-svg": "^12.1.1"
  },
  "peerDependencies": {
    "@fluentui/react-context-selector": "*",
    "react": "*",
    "react-i18next": "*",
    "react-native": "*",
    "react-native-gesture-handler": "*",
    "react-native-reanimated": "*"
  }
}
