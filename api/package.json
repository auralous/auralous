{
  "name": "@auralous/api",
  "version": "0.0.0",
  "description": "APIs for Auralous",
  "scripts": {
    "codegen": "graphql-codegen",
    "build:babel": "babel src -d dist --extensions \".ts\"",
    "build:dts": "tsc",
    "build": "yarn codegen && yarn build:dts && yarn build:babel",
    "build:watch": "concurrently \"yarn codegen --watch\" \"yarn build:dts --watch\" \"yarn build:babel -w\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist && rm tsconfig.tsbuildinfo"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/auralous/auralous.git",
    "directory": "api"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auralous/auralous/issues"
  },
  "homepage": "https://github.com/auralous/auralous#readme",
  "dependencies": {
    "@urql/exchange-auth": "^0.1.6",
    "@urql/exchange-graphcache": "^4.3.2",
    "@urql/exchange-persisted-fetch": "^1.3.2",
    "graphql": "^15.5.1",
    "graphql-ws": "^5.4.1",
    "urql": "^2.0.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/typescript": "^2.1.2",
    "@graphql-codegen/typescript-operations": "^2.1.2",
    "@graphql-codegen/typescript-urql": "^3.1.2",
    "@graphql-codegen/typescript-urql-graphcache": "^2.1.2",
    "@graphql-codegen/urql-introspection": "^2.1.0"
  }
}
