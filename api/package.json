{
  "name": "@auralous/api",
  "version": "0.0.0",
  "description": "APIs for Auralous",
  "homepage": "https://github.com/auralous/auralous#readme",
  "bugs": {
    "url": "https://github.com/auralous/auralous/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/auralous/auralous.git",
    "directory": "api"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn codegen && yarn build:dts && yarn build:js",
    "build:js": "babel src -d dist --extensions \".ts\"",
    "build:dts": "tsc",
    "build:watch": "concurrently \"yarn codegen --watch\" \"yarn build:dts -w\" \"yarn build:js -w\"",
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "codegen": "graphql-codegen",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "sideEffects": false,
  "dependencies": {
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-auth": "^0.1.7",
    "@urql/exchange-graphcache": "^4.3.6",
    "@urql/exchange-persisted-fetch": "^1.3.4",
    "graphql": "^16.3.0",
    "graphql-ws": "^5.5.5",
    "urql": "^2.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/preset-typescript": "^7.16.7",
    "@graphql-codegen/cli": "^2.5.0",
    "@graphql-codegen/typescript": "^2.4.3",
    "@graphql-codegen/typescript-operations": "^2.2.4",
    "@graphql-codegen/typescript-urql": "^3.5.1",
    "@graphql-codegen/typescript-urql-graphcache": "^2.2.4"
  },
  "peerDependencies": {}
}
