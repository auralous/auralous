{
  "name": "@auralous/app",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn build:dts && yarn build:babel",
    "build:babel": "babel src -d dist --extensions \".ts,.tsx\" --copy-files",
    "build:dts": "tsc",
    "build:watch": "concurrently \"yarn build:dts --watch --preserveWatchOutput\" \"yarn build:babel -w\"",
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "postinstall": "patch-package"
  },
  "dependencies": {
