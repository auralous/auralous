  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn build:dts && yarn build:babel",
    "build:babel": "babel src -d dist --source-maps --extensions \".ts,.tsx\" --copy-files",
